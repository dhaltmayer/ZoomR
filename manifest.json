{
	"manifest_version" : 2,
	"name": "ZoomR",
	"version": "0.1.5",
	"author": "David H. Altmayer",
	"minimum_chrome_version": "39",
	
	"description": "Zoom in on articles.",
	"content_scripts" : [{
		"matches": ["http://*/*"],
		"js": ["content.js"]
		}],
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [ "tabs", "contextMenus"]
}
